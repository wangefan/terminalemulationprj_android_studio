<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyWidth="20%p"
    android:keyHeight="7%p">
    <Row android:keyWidth="17%p">
        <Key android:codes="-12" android:keyLabel="abc" android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="-13" android:keyLabel="!@#" android:isSticky="true" />
        <Key android:codes="-14" android:keyLabel="Func"/>
        <Key android:codes="-15" android:keyLabel="Server"/>
        <Key android:codes="-16" android:keyIcon="@drawable/sym_keyboard_done"/>
        <Key android:codes="-17" android:keyIcon="@drawable/sym_sys_keyboard_up" android:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key android:keyLabel="F1" android:keyEdgeFlags="left"/>
        <Key android:keyLabel="F2"/>
        <Key android:keyLabel="F3"/>
        <Key android:keyLabel="F4"/>
        <Key android:keyLabel="F5" android:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key android:keyLabel="F6" android:keyEdgeFlags="left"/>
        <Key android:keyLabel="F7"/>
        <Key android:keyLabel="F8"/>
        <Key android:keyLabel="F9"/>
        <Key android:keyLabel="F10" android:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key android:keyLabel="F11" android:keyEdgeFlags="left"/>
        <Key android:keyLabel="F12"/>
        <Key android:keyLabel="F13"/>
        <Key android:keyLabel="F14"/>
        <Key android:keyLabel="F15" android:keyEdgeFlags="right"/>
    </Row>
    <Row android:rowEdgeFlags="bottom">
        <Key android:keyLabel="F16" android:keyEdgeFlags="left"/>
        <Key android:keyLabel="F17"/>
        <Key android:keyLabel="F18"/>
        <Key android:keyLabel="F19"/>
        <Key android:keyLabel="F20" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>