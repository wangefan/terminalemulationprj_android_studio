<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/vt_good_feedback">
        <ListPreference
            android:key="@string/fb_good_key"
            android:title="@string/fb_type_title"
            android:entries="@array/fb_array"
            android:entryValues="@array/fb_array_vals"
            android:negativeButtonText="@null"
            android:positiveButtonText="@null" />
        <Preference
            android:key="@string/fb_good_content_key"/>
    </PreferenceCategory>
    <PreferenceCategory
        android:title="@string/vt_error_feedback">
        <ListPreference
            android:key="@string/fb_error_key"
            android:title="@string/fb_type_title"
            android:entries="@array/fb_array"
            android:entryValues="@array/fb_array_vals"
            android:negativeButtonText="@null"
            android:positiveButtonText="@null" />
        <Preference
            android:key="@string/fb_error_content_key"/>
    </PreferenceCategory>
</PreferenceScreen>
